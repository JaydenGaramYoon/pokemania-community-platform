{
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/preset-env": "^7.27.2",
        "babel-loader": "^10.0.0",
        "newman": "^6.2.1",
        "nodemon": "^3.1.10"
    },
    "name": "week-3-mern_skeleton",
    "version": "1.0.0",
    "type": "module",
    "description": "First version",
    "main": "server.js",
    "author": "David Suarez",
    "license": "ISC",
    "scripts": {
        "dev": "concurrently 'vite' \"nodemon server.js\"",
        "build": "vite build",
        "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "api:test:auth": "newman run \"api/postman/Auth API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:user": "newman run \"api/postman/User API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:favourites": "newman run \"api/postman/Favourites API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:game": "newman run \"api/postman/Game API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:talktalk": "newman run \"api/postman/TalkTalk API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:admin": "newman run \"api/postman/Admin API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:profile": "newman run \"api/postman/Profile API.postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:public": "newman run \"api/postman/Public API (Search).postman_collection.json\" -e \"api/postman/environments/dev.postman_environment.json\"",
        "api:test:all": "npm run api:test:auth & npm run api:test:user & npm run api:test:favourites & npm run api:test:game & npm run api:test:talktalk & npm run api:test:admin & npm run api:test:profile & npm run api:test:public"
    },
    "dependencies": {
        "body-parser": "^2.2.0",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "express-jwt": "^8.5.1",
        "global": "^4.4.0",
        "helmet": "^8.1.0",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "mocha": "^11.7.1",
        "mongoose": "^8.16.1",
        "selenium-webdriver": "^4.34.0"
    }
}
